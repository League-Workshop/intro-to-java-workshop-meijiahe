package section2;

import java.awt.Color;
import org.jointheleague.graphical.robot.Robot;

public class MyFirstJavaProgram {
	
	public static void main(String[] args) {
		
		// START HERE
Robot Salmon=new Robot(); 
Salmon.setPenColor(40,30,30);
Salmon.penDown();
Salmon.move(120);
Salmon.turn(90);
Salmon.setSpeed(500);
Salmon.move(120);
Salmon.turn(90);
Salmon.turn(180);
Salmon.move(100);
Salmon.turn(180);
Salmon.move(100);
	}
}
